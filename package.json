  },
  "devDependencies": {
    "@redhat-actions/action-io-generator": "^1.5.0",
    "@types/node": "^20.11.24",
    "@types/sarif": "^2.1.7",
    "@vercel/ncc": "^0.38.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@actions/artifact": "^2.1.4",
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
